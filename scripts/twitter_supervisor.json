{  
   "type":"kafka",
   "dataSchema":{  
      "dataSource":"twitter-kafka",
      "parser":{  
         "type":"string",
         "parseSpec":{  
            "format":"json",
            "timestampSpec":{  
               "column":"timestamp",
               "format":"iso"
            },
            "dimensionsSpec":{  
               "dimensions":[  
                  "id",
                  "timestamp",
                  "sentiment",
                  "keyPhrases",
                  "user",
                  "text",
                  "source"
               ]
            }
         }
      },
      "metricsSpec":[  
         {  
            "name":"tweets",
            "type":"count"
         },
         {  
            "name":"sentiment_sum",
            "type":"doubleSum",
            "fieldName":"sentiment"
         }
      ],
      "granularitySpec":{  
         "type":"uniform",
         "segmentGranularity":"MINUTE",
         "queryGranularity":"NONE",
         "rollup":true
      }
   },
   "ioConfig":{  
      "topic":"twitter_output",
      "consumerProperties":{  
         "bootstrap.servers":"localhost:9092"
      },
      "useEarliestOffset":true
   }
}